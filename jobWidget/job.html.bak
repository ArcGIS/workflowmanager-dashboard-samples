<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title></title>
</head>
<body>
    <div data-dojo-type="extension/JobWidget"></div>
    <script>
        var path = location.pathname.replace(/[^\/]+$/, '');
        var dojoConfig = {
            async: true,
            paths: {
                "extension": path
            },
            packages: [ {
                name: "workflowmanager",
                location: path + "../workflowmanager"
            }]
        };
    </script>
    <script src="//js.arcgis.com/3.15/"></script>
    <script type="text/javascript" src="//code.jquery.com/jquery-1.6.min.js"></script>
    <script>
        require([
            "dojo/parser",
            "dojo/domReady!"
        ], function(parser){
            parser.parse();
        });
    </script>

</body>
</html>